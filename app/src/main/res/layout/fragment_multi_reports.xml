<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
 android:layout_height="match_parent"
    android:layout_width="match_parent">

<androidx.coordinatorlayout.widget.CoordinatorLayout
    android:id="@+id/coordinator_content"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:fitsSystemWindows="true"
    tools:context=".MainActivity">

    <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:id="@+id/linearLayout"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/efec8174"
        android:gravity="center_vertical"
        android:orientation="vertical"
        android:paddingTop="80dp"
        tools:context=".ui.fragments.FlatStartFragment"/>

    <com.evrencoskun.tableview.TableView
        android:id="@+id/tableview"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/table_test_container"

        ><!--app:corner_view_location="top_right" -->

    </com.evrencoskun.tableview.TableView>
</androidx.coordinatorlayout.widget.CoordinatorLayout>
    <TextView
        android:id="@+id/tv_table_header"
        android:textAlignment="center"
        android:layout_gravity="center_horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/table_header_text"/>

    <ImageButton

        android:id="@+id/b_anal_back"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="end"
        android:layout_margin="1dp"
        android:background="@android:drawable/ic_menu_send"
        android:contentDescription="дальше" />

    <ImageButton

        android:id="@+id/b_anal_prev"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="start"
        android:layout_margin="1dp"
        android:rotation="180"
        android:background="@android:drawable/ic_menu_send"
        android:contentDescription="вернуться" />
    </FrameLayout>